<% if(typeof dbData != 'undefined'){%>
<table id="dataTable">
    <% Object.keys(dbData[0]).forEach( header => {%>
        <th id="dataTableHeader"> <%= header; %> </th>
    <% }); %>
    <% dbData.forEach( row => {%>
    <tr>
        <% Object.values(row).forEach( value => {%>
        <td> 
            <%= value; %>
        </td>
        <% }); %>
    </tr>
    <% }); %>
</table>
<% } %>